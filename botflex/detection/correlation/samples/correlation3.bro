function hz_scan(index: Correlation::Index, val: Correlation::Val)
	{
	local hz_stream: Correlation::Stream;
	local stream_name = "HZ_SCAN";
	hz_stream = [ $name=stream_name, $alt_name=stream_name, $weight=0.8 ];
	Correlation::add_stream( corr_id, filter_v_rule1b, index, hz_stream );
	Correlation::add_stream( corr_id, filter_v_rule2b, index, hz_stream );
	Correlation::add_stream( corr_id, filter_v_rule3b, index, hz_stream );
	Correlation::add_stream( corr_id, filter_v_rule4b, index, hz_stream );
	Correlation::add_stream( corr_id, filter_v_rule5b, index, hz_stream );
	Correlation::add_stream( corr_id, filter_v_rule6b, index, hz_stream );

	Correlation::add_stream( corr_id, filter_v_rule1bi, index, hz_stream );
	Correlation::add_stream( corr_id, filter_v_rule2bi, index, hz_stream );
	Correlation::add_stream( corr_id, filter_v_rule3bi, index, hz_stream );
	Correlation::add_stream( corr_id, filter_v_rule4bi, index, hz_stream );
	Correlation::add_stream( corr_id, filter_v_rule5bi, index, hz_stream );
	Correlation::add_stream( corr_id, filter_v_rule6bi, index, hz_stream );
	Correlation::add_stream( corr_id, filter_v_rule7bi, index, hz_stream );
	}

function hz_spam(index: Correlation::Index, val: Correlation::Val)
