event Exploit::exploit( victim: addr, weight: double, detailed_name: string )
	{
	local my_index: Correlation::Index;
	my_index = [$host=victim];

	local v_stream: Correlation::Stream;
	local stream_name = "EXPLOIT";
	v_stream = [ $name=stream_name, $alt_name=detailed_name, $weight=weight ];
	#Correlation::add_stream( corr_id, filter_v_rule1, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule2, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule3, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule4, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule5, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule6, my_index, v_stream );

	Correlation::add_stream( corr_id, filter_v_rule1i, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule2i, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule3i, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule4i, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule5i, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule6i, my_index, v_stream );

	Correlation::add_stream( corr_id, filter_v_rule1b, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule2b, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule3b, my_index, v_stream );	
	Correlation::add_stream( corr_id, filter_v_rule4b, my_index, v_stream );	
	Correlation::add_stream( corr_id, filter_v_rule5b, my_index, v_stream );	
	Correlation::add_stream( corr_id, filter_v_rule6b, my_index, v_stream );	

	Correlation::add_stream( corr_id, filter_v_rule1bi, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule2bi, my_index, v_stream );
	Correlation::add_stream( corr_id, filter_v_rule3bi, my_index, v_stream );	
	Correlation::add_stream( corr_id, filter_v_rule4bi, my_index, v_stream );	
	Correlation::add_stream( corr_id, filter_v_rule5bi, my_index, v_stream );	
	Correlation::add_stream( corr_id, filter_v_rule6bi, my_index, v_stream );	
	}

