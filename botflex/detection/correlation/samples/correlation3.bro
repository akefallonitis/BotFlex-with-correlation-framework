event bro_init()
	{
	local done = Correlation::add_correlation_item(corr_id, [$name=filter_hz_attack_scan_ip_ob,
	                                       $every=15mins,
					       $rule=";;{ATTACK_SCAN_IP_OB | ATTACK_SCAN_IP_OB_CRITICAL }",
	                                       $correlated=hz_scan,
					       $alt_name_in_history = T,
					       $filter_type="horizontal"]); 
	
	if ( !done )
		print "Correlation Framework Error: Failed to add filter hz_attack_scan_ip_ob.";

	#==================================

	done = Correlation::add_correlation_item(corr_id, [$name=filter_hz_attack_spam,
	                                       $every=15mins,
					       $rule=";;{ATTACK_SPAM_HIGH_SMTP | ATTACK_SPAM_MX }",
	                                       $correlated=hz_spam,
					       $alt_name_in_history = T,
					       $filter_type="horizontal"]); 
	
	if ( !done )
		print "Correlation Framework Error: Failed to add filter hz_attack_spam.";

	#==================================

	done = F;

	local v_rule1i = "; ;{ CNC_OTHER & ( EXPLOIT | EGG | ATTACK ) }";
	
	done = Correlation::add_correlation_item(corr_id, [$name=filter_v_rule1i,
	                                       $every=wnd_correlation,
					       $rule=v_rule1i,
	                                       #$correlated=bot_found,
					       $alt_name_in_history = T,
					       $filter_type="vertical"]); 

	if ( !done )
		print "Correlation Framework Error: Failed to add filter v_rule1i.";

	#=================================

	done = F;

	local v_rule2 = "; ;{ CNC_BLACKLIST | (CNC_OTHER & ( EXPLOIT | EGG | ATTACK )) }";
	
